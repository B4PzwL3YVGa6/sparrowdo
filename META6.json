{
  "perl"        : "6.*",
  "name"        : "Sparrowdo",
  "version"     : "0.0.17",
  "description" : "Configuration management tool based on sparrow plugins system",
  "authors" : [ "Alexey Melezhik"],
  "depends"     : [
                    "Terminal::ANSIColor",
                    "JSON::Tiny"
                  ],
  "source-type" : "git",
  "support" : {
