        "source" : "https://github.com/melezhik/sparrowdo.git"
    },
   "provides" : {
        "Sparrowdo" : "lib/Sparrowdo.pm6",
        "Sparrowdo::Core::DSL::User" : "lib/Sparrowdo/Core/DSL/User.pm6",
